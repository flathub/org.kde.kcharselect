                    "sha256": "dd1502427ae86c04b93d53d4c2ba9069a60cb52676d42010b4031d1908218e50",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 1486,
                        "stable-only": true,
                        "url-template": "https://download.kde.org/stable/release-service/$version/src/kcharselect-$version.tar.xz"
                    }
                }
            ]
        }
